{
  "name": "web",
  "version": "0.0.1",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "astro dev",
    "build": "dmno run -- astro build",
    "preview": "dmno run -- astro preview",
    "astro": "dmno run -- astro",
    "check-types": "tsc --noEmit",
    "test": "dmno run -- playwright test",
    "test:ui": "dmno run -- playwright test --ui",
    "env:docker": "dmno resolve --service web --format env > .dmno/.env.docker",
    "lint": "eslint . --fix --cache --max-warnings 0",
    "test:snapshots": "pnpm run test -- --update-snapshots"
  },
  "dependencies": {
    "@astrojs/node": "*",
    "@astrojs/react": "*",
    "@clerk/astro": "*",
    "@dmno/astro-integration": "*",
    "@nanostores/react": "*",
    "@radix-ui/react-avatar": "*",
    "@radix-ui/react-dialog": "*",
    "@radix-ui/react-popover": "*",
    "@radix-ui/react-select": "*",
    "@radix-ui/react-slider": "*",
    "@radix-ui/react-slot": "*",
    "@repo/ts-rest": "workspace:*",
    "@tailwindcss/vite": "*",
    "@tanstack/react-query": "*",
    "@tanstack/react-table": "*",
    "@ts-rest/core": "*",
    "@ts-rest/react-query": "*",
    "astro": "*",
    "class-variance-authority": "*",
    "clsx": "*",
    "cmdk": "*",
    "date-fns": "*",
    "dmno": "*",
    "lucide-react": "*",
    "nanostores": "*",
    "react": "*",
    "react-dom": "*",
    "react-error-boundary": "*",
    "tailwind-merge": "*",
    "tailwindcss": "^4.0.15",
    "tw-animate-css": "*"
  },
  "devDependencies": {
    "@astrojs/ts-plugin": "*",
    "@axe-core/playwright": "*",
    "@playwright/test": "*",
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/react": "*",
    "@types/react-dom": "*",
    "eslint": "*"
  }
}
